{
  "name": "pure_vue_cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "percy": "build-storybook && percy-storybook --widths=320,1280",
    "build:cio": "vue-cli-service build",
    "build:storybook": "build-storybook -s public",
    "serve:storybook": "start-storybook -p 6006 --debug-webpack",
    "ci:cy:cio": "yarn dev & wait-on http://localhost:8080 && yarn cy:headless:cio",
    "ci:cy:storybook": "yarn serve:storybook --ci & wait-on http://localhost:6006 && yarn cy:headless:storybook",
    "cy:gui": "npx cypress open --config fileServerFolder=tests/e2e",
    "cy:headless:storybook": "npx cypress run --config fileServerFolder=tests/e2e --spec tests/e2e/specs/storybook/**/*",
    "cy:headless:cio": "npx cypress run --config fileServerFolder=tests/e2e --spec 'tests/e2e/specs/app/**/*'",
    "deploy:storybook": "firebase deploy --only hosting:storybook",
    "deploy:cio": "firebase deploy --only hosting:cio"
  },
  "dependencies": {
    "body-scroll-lock": "2.6.4",
    "clipboard-copy": "3.1.0",
    "core-js": "3.6.4",
    "date-fns": "2.10.0",
