    "firebaseui": "4.4.0",
    "lodash.omit": "4.5.0",
    "trianglify": "2.0.0",
    "vue": "2.6.11",
    "vue-router": "3.1.5",
    "vue-the-mask": "0.11.1",
    "vuetify": "2.2.11",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
