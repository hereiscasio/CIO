{
	"database": {
		"rules": "database.rules.json"
	  },
	"hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
		{
			"source": "/user/**",
			"function": "app"
		},
		{
			"source": "**",
			"destination": "/index.html"
		}
    ]
  }
}
